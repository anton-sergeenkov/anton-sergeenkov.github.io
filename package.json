{
  "name": "lets-code",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "gh-pages": "gh-pages -d out -t true",
    "commit": "git add . && git commit -m \"Bash Script Commit\" && git push origin head",
    "create-nojekyll": "node ./node.js/nojekyll.js",
    "copy-cv": "node ./node.js/copy-cv.js",
    "deploy": "npm run build && npm run create-nojekyll && npm run copy-cv && npm run gh-pages && npm run commit",
    "format": "prettier --write .",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.16.6",
    "@mui/material": "^5.16.6",
    "classnames": "^2.5.1",
    "dotenv": "^16.4.5",
    "esm": "^3.2.25",
    "next": "^14.2.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.5",
    "eslint-plugin-prettier": "^5.2.1",
    "gh-pages": "^6.1.1",
    "prettier": "^3.3.3",
    "prettier-config-nick": "^1.0.8",
    "typescript": "^5.5.4"
  },
  "prettier": "prettier-config-nick"
}
